	Doi = {https://doi.org/0.1039/C4MB00410H},
	Issue = {12},
	Journal = {Mol. BioSyst.},
	Pages = {3147-3154},
	Publisher = {The Royal Society of Chemistry},
	Title = {Predicting protein-protein interactions between human and hepatitis C virus via an ensemble learning method},
	Url = {http://dx.doi.org/10.1039/C4MB00410H},
	Volume = {10},
	Year = {2014},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://pubs.rsc.org/en/content/getauthorversionpdf/C4MB00410H},
